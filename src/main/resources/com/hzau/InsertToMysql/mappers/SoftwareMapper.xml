<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hzau.InsertToMysql.mappers.SoftwareMapper">

    <select id="selectByName" resultType="com.hzau.InsertToMysql.domain.Software">
        select *
        from biology.t_software
        where SOFTWARE_NAME = #{SOFTWARE_NAME};
    </select>
</mapper>